{
  "name": "canturi-website",
  "version": "1.0.0",
  "description": "Canturi Shopify Website",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write '**/*' --ignore-unknown",
    "deploy": "theme deploy --allow-live",
    "watch": "sass --watch --style=compressed assets/theme.scss:assets/theme.css",
    "dev": "theme watch --allow-live",
    "build": "sass --style=compressed assets/theme.scss assets/theme.css",
    "start": "(npm run watch --allow-live) & theme watch --allow-live",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renansigolo/canturi-website.git"
  },
  "keywords": [
    "canturi",
    "shopify"
  ],
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renansigolo/canturi-website/issues"
  },
  "homepage": "https://github.com/renansigolo/canturi-website#readme",
  "dependencies": {
    "sass": "1.69.5"
  },
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "1.3.4",
    "prettier": "3.1.0"
  },
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid"
    ]
  }
}
